{
  "name": "tutu/wsdlgenerator",
  "description": "Simple library for generating php dto classes from a wsdl file.",
  "keywords": [
    "soap",
    "wsdl",
    "wsdl2php"
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=5.3.0",
    "ext-soap": "*",
    "psr/log": "*",
    "symfony/options-resolver": "~2.6|~3.0",
    "symfony/polyfill-iconv": "^1.2"
  },
  "require-dev": {},
  "autoload": {
    "psr-4": {
      "Tutu\\Wsdl2PhpGenerator\\": "src/"
    }
  }
}
